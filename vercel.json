{
	"version": 2,
	"builds":
	[
		{
			"use": "@vercel/node",
			"src": "dist/apps/api/main.js"
		},
		{
			"use": "@vercel/static",
			"src": "dist/apps/angular-nest/**"
		}
	],
	"routes":
	[
		{
			"src": "/api(.*)",
			"dest": "dist/apps/api/main.js"
		},
		{
			"src": "/",
			"dest": "dist/apps/angular-nest/index.html"
		},
		{
			"src": "/(.*)",
			"dest": "dist/apps/angular-nest/$1"
		}
	]
}

